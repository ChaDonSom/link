        "cross-env": "^5.2.1",
        "flatpickr": "^4.6.3",
        "jquery": "^3.2",
        "laravel-mix": "^4.1.4",
        "lodash": "^4.17.15",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.23.0",
        "sass-loader": "^7.3.1",
        "vue": "^2.5.17",
        "vue-router": "^3.1.3",
        "vue-template-compiler": "^2.6.10",
        "vuex": "^3.1.1"
    },
    "dependencies": {
